        <periods>0.0 0.025 0.1 0.2</periods>

        <uhs>
            <gml:Point>
                <gml:pos>0.0 0.0</gml:pos>
            </gml:Point>
            <IMLs>0.3 0.5 0.2 0.1</IMLs>
        </uhs>
        <uhs>
            <gml:Point>
                <gml:pos>0.0 1.0</gml:pos>
            </gml:Point>
            <IMLs>0.3 0.5 0.2 0.1</IMLs>
        </uhs>
    </uniformHazardSpectra>
</nrml>
