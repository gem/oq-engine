<?xml version="1.0" encoding="utf-8"?>
<nrml xmlns="http://openquake.org/xmlns/nrml/0.5"
      xmlns:gml="http://www.opengis.net/gml" >
    <sourceModel name="Example Source Model Containing a Simple Fault Source">
        <sourceGroup name="group 1" tectonicRegion="Active Shallow Crust">
            <simpleFaultSource id="3" name="Simple Fault Source"
                tectonicRegion="Active Shallow Crust">
                <simpleFaultGeometry>
                    <gml:LineString>
                        <gml:posList>
                            1.0 -0.2 1.4 0.0 1.7 0.0
                        </gml:posList>
                    </gml:LineString>
                    <dip> 30 </dip>
                    <upperSeismoDepth> 5.0 </upperSeismoDepth>
                    <lowerSeismoDepth> 15.0 </lowerSeismoDepth>
                </simpleFaultGeometry>
                <magScaleRel> WC1994 </magScaleRel>
                <ruptAspectRatio> 2.0000000E+00 </ruptAspectRatio>
                <truncGutenbergRichterMFD slipRate="5.4" rigidity="32" bValue="0.9" maxMag="7.0" minMag="5.5"/>
                <rake> 90 </rake>
            </simpleFaultSource>
        </sourceGroup>
    </sourceModel>
</nrml>
