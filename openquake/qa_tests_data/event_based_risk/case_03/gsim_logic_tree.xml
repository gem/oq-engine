            </logicTreeBranchSet>
        </logicTreeBranchingLevel>
	    <logicTreeBranchingLevel branchingLevelID="bl2">
			<logicTreeBranchSet uncertaintyType="gmpeModel" branchSetID="bs2"
                    applyToTectonicRegionType="Stable Shallow Crust">
                <logicTreeBranch branchID="b3">
                    <uncertaintyModel>AtkinsonBoore2006</uncertaintyModel>
                    <uncertaintyWeight>0.7</uncertaintyWeight>
                </logicTreeBranch>
                <logicTreeBranch branchID="b4">
                    <uncertaintyModel>AkkarBommer2010</uncertaintyModel>
                    <uncertaintyWeight>0.3</uncertaintyWeight>
                </logicTreeBranch>
            </logicTreeBranchSet>
        </logicTreeBranchingLevel>
