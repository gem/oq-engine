      <logicTreeBranch branchID="b21">
        <uncertaintyModel>AkkarBommer2010</uncertaintyModel>
        <uncertaintyWeight>1</uncertaintyWeight>
      </logicTreeBranch>

    </logicTreeBranchSet>
  </logicTreeBranchingLevel>
  
