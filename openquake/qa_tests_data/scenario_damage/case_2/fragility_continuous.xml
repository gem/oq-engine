<?xml version='1.0' encoding='utf-8'?>
<nrml xmlns:gml="http://www.opengis.net/gml"
      xmlns="http://openquake.org/xmlns/nrml/0.5">

     <fragilityModel format="continuous">
        <description>Fragility model for QA test</description>

        <limitStates>LS1 LS2</limitStates>
        <ffs type="lognormal">
            <taxonomy>RC</taxonomy>
            <IML IMT="PGA" minIML="0.1" maxIML="9.9" imlUnit="g"/>
            <ffc ls="LS1">
                <params mean="0.2" stddev="0.05" />
            </ffc>
            <ffc ls="LS2">
                <params mean="0.35" stddev="0.10" />
            </ffc>
        </ffs>
        <ffs type="lognormal">
            <taxonomy>RM</taxonomy>
            <IML IMT="PGA" minIML="0.1" maxIML="9.9" imlUnit="g"/>
            <ffc ls="LS1">
                <params mean="0.25" stddev="0.08" />
            </ffc>
            <ffc ls="LS2">
                <params mean="0.40" stddev="0.12" /> 
            </ffc>
        </ffs>
    </fragilityModel>
</nrml>

