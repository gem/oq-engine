<?xml version="1.0" encoding="UTF-8"?>
<nrml xmlns:gml="http://www.opengis.net/gml"
      xmlns="http://openquake.org/xmlns/nrml/0.4">
    <logicTree logicTreeID="lt1">

            <logicTreeBranchSet uncertaintyType="gmpeModel" branchSetID="bs1"
                    applyToTectonicRegionType="Non_cratonic">

                <logicTreeBranch branchID="NGAEastUSGSSeedPEER_GP_indirect">
                    <uncertaintyModel>
                        [GmpeIndirectAvgSA]
                        gmpe_name = "NGAEastUSGSSeedPEER_GP"
                        t_low = 0.5
                        t_high = 2.0
                        n_per = 10
                        corr_func="eshm20"
                    </uncertaintyModel>
                    <uncertaintyWeight>0.25</uncertaintyWeight>
                </logicTreeBranch>

                <logicTreeBranch branchID="NGAEastAUS23_vs3000_indirect">
                    <uncertaintyModel>
                        [GmpeIndirectAvgSA]
                        gmpe_name = "NGAEastAUS23_vs3000"
                        t_low = 0.5
                        t_high = 2.0
                        n_per = 10
                        corr_func="eshm20"
                    </uncertaintyModel>
                    <uncertaintyWeight>0.25</uncertaintyWeight>
                </logicTreeBranch>

                <logicTreeBranch branchID="NGAEastUSGSSeedPEER_GP_generic">
                    <uncertaintyModel>
                        [GenericGmpeAvgSA]
                        gmpe_name = "NGAEastUSGSSeedPEER_GP"
                        avg_periods = [0.5, 1.0, 2.0]
                        corr_func="eshm20"
                    </uncertaintyModel>
                    <uncertaintyWeight>0.25</uncertaintyWeight>
                </logicTreeBranch>

                <logicTreeBranch branchID="NGAEastAUS23_vs3000_generic">
                    <uncertaintyModel>
                        [GenericGmpeAvgSA]
                        gmpe_name = "NGAEastAUS23_vs3000"
                        avg_periods = [0.5, 1.0, 2.0]
                        corr_func="eshm20"
                    </uncertaintyModel>
                    <uncertaintyWeight>0.25</uncertaintyWeight>
                </logicTreeBranch>

           </logicTreeBranchSet>
    </logicTree>
</nrml>
