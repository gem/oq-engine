            </ffd>
            <ffd ls="LS2">
                <poE>0.001 0.02 0.25 0.72</poE>
            </ffd>
        </ffs>
        <ffs gml:id="ff02">
            <taxonomy>RM</taxonomy>
            <ffd ls="LS1">
                <poE>0.01 0.64 0.95 1.0</poE>
            </ffd>
            <ffd ls="LS2">
                <poE>0.0003 0.05 0.40 0.86</poE>
            </ffd>
