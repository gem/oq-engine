#!/bin/sh
# preinst script for openquake
#
# see: dh_installdeb(1)

#set -x
set -e

for dir in  /usr/share/pyshared/openquake /usr/share/doc/python-oq /usr/share/pyshared/openquake*egg-info
do
    rm -rf $dir
done

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
