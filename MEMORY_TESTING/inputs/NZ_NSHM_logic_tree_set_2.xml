                    </uncertaintyModel>
                    <uncertaintyWeight>0.33</uncertaintyWeight>
                </logicTreeBranch>

                <logicTreeBranch branchID= "P_SS-GLO">
                    <uncertaintyModel>[ParkerEtAl2020SSlab]
